#NESTED LOOPS!!!
for i in range(4):
    for j in range (5):
        print(i,j)

#Example 1: Multiplication table        
for i in range(1, 11):
    for j in range(1, 11):
        print(i, "x", j, "=", i * j)
        
#Example 2: Printing patterns
for i in range(1, 8):
    for j in range(1, 5):
        print("*", end="")
    print()
